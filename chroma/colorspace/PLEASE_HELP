I need help: these two give different results!


// C code:
void Ycbcr2Rgb(double *R, double *G, double *B, double Y, double Cr, double Cb)
{
	Y -= 16;
	Cb -= 128;
	Cr -= 128;
	*R = (double)(0.00456621004566210107*Y + 1.1808799897946415e-09*Cr + 0.00625892896994393634*Cb);
	*G = (double)(0.00456621004566210107*Y - 0.00153632368604490212*Cr - 0.00318811094965570701*Cb);
	*B = (double)(0.00456621004566210107*Y + 0.00791071623355474145*Cr + 1.1977497040190077e-08*Cb);
}

// Go code:
func Ycbcr2Rgb(y, cb, cr float64) (r, g, b float64) {
	y -= 16
	cb -= 128
	cr -= 128
	r = 0.00456621004566210107*y + 1.1808799897946415e-9*cr + 0.00625892896994393634*cb
	g = 0.00456621004566210107*y - 0.00153632368604490212*cr - 0.00318811094965570701*cb
	b = 0.00456621004566210107*y + 0.00791071623355474145*cr + 1.1977497040190077e-8*cb
	return
}

